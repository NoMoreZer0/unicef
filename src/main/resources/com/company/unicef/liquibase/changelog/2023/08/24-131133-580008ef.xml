<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.11.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="unicef">
        <createTable tableName="NEEDS_HEALTH">
            <column name="ID" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_NEEDS_HEALTH"/>
            </column>
            <column name="UUID" type="UUID"/>
            <column name="NAME" type="CLOB"/>
        </createTable>
    </changeSet>
    <changeSet id="2" author="unicef">
        <dropColumn columnName="needs_health" tableName="second_form_check_box"/>
    </changeSet>
</databaseChangeLog>